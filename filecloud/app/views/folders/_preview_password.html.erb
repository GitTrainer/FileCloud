<%= form_tag(:controller =>'filestreams', :action =>'create_unlocked', :id =>'preivew_form', :remote => true) do %>
  <div class="field">
    <p>This file is password protected. Please enter the password here to unlock it. </p>
  <%= password_field_tag :password_protect,params[:password_protect], :autofocus => true, :autocomplete => 'off', :placeholder => 'Enter password' , :title => "Enter key" %>
  </div>
  <div class="actions">
    
  <input type="submit" value="Unlock" id="btn_preivew"/>
  </div>
  <%= hidden_field_tag :file_id, file_id %>
<% end %>
