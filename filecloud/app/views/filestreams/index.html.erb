<h1>UpLoadFile to <%=@folder.name%></h1>
<br/>
<%= form_for(@fileupload,:html => { :multipart => true }) do |f| %>
    <p>
      <%= f.hidden_field :folder_id,:value=> params[:folder_id] %>
      <%= f.label :FileUpLoad %>
      
     </p>
     <p>
      <%= f.submit "Upload" %>
    <% end %>
    </p>

